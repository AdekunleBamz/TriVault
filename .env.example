# TriVault Environment Variables
# Copy this file to .env and fill in your values

# Your private key for deploying contracts (with 0x prefix)
PRIVATE_KEY=your_private_key_here

# Your wallet address for receiving creator fees
CREATOR_FEE_ADDRESS=your_wallet_address_here

# Base RPC URLs
BASE_MAINNET_RPC=https://mainnet.base.org
BASE_SEPOLIA_RPC=https://sepolia.base.org

# Optional: BaseScan API key for contract verification
BASESCAN_API_KEY=your_basescan_api_key

# App Config
NEXT_PUBLIC_CREATOR_FEE=0.00001
NEXT_PUBLIC_CHAIN_ID=8453

# Farcaster (for mini-app)
NEXT_PUBLIC_APP_URL=http://localhost:3000
